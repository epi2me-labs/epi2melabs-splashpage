{
  "name": "@epi2melabs/epi2melabs-splashpage",
  "version": "0.1.1",
  "description": "Accompanies the EPI2ME labs experience. Not for general use.",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://github.com/epi2me-labs/epi2melabs-splashpage",
  "bugs": {
    "url": "https://github.com/epi2me-labs/epi2melabs-splashpage"
  },
  "license": "BSD-3-Clause",
  "author": "trich",
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "schema/**/*.json",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "repository": {
    "type": "git",
    "url": "https://github.com/epi2me-labs/epi2melabs-splashpage"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf lib tsconfig.tsbuildinfo",
    "eslint": "eslint . --ext .ts,.tsx --fix",
    "eslint:check": "eslint . --ext .ts,.tsx",
    "prepare": "jlpm run clean && jlpm run build",
    "watch": "tsc -w"
  },
  "dependencies": {
    "@jupyterlab/application": "^2.0.0",
    "@jupyterlab/cells": "^2.2.2",
    "@jupyterlab/docmanager": "^2.2.3",
    "@jupyterlab/launcher": "^2.2.5",
    "@jupyterlab/notebook": "^2.2.5",
    "@jupyterlab/settingregistry": "^2.2.4",
    "@jupyterlab/ui-components": "^2.2.3",
    "@lumino/widgets": "^1.14.0",
    "moment": "^2.29.1"
  },
  "devDependencies": {
    "@types/webpack-env": "^1.15.3",
    "@typescript-eslint/eslint-plugin": "^2.25.0",
    "@typescript-eslint/parser": "^2.25.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.2",
    "prettier": "1.16.4",
    "rimraf": "^2.6.1",
    "typescript": "~3.7.0"
  },
  "sideEffects": [
    "style/*.css"
  ],
  "jupyterlab": {
    "extension": true,
    "schemaDir": "schema"
  },
  "publishConfig": {
    "access": "public"
  }
}
